var adString2="http://" + "ad.doubleclick.net/N3618/adj/mcrst/;mcrst=junipernative300x250io122059li1111788;ord=" + ord + "?";
</script> 
  <div class="adContainer1">
   <noscript>
    <a href="http://ad.doubleclick.net/N3618/jump/mcrst/;mcrst=junipernative300x250io122059li1111788;sz=300x250;ord=123456789?" target="_blank"><img src="http://ad.doubleclick.net/N3618/ad/mcrst/;mcrst=junipernative300x250io122059li1111788;sz=300x250;ord=123456789?" border="0" alt="" /></a> 
   </noscript>
  </div> 
  <script type="text/javascript">
$(document).ready(function() {
$(".adContainer1").writeCapture().html("<scr"+"ipt type='text/javascript' src='"+adString2+"'></scr"+"ipt>");
