	function contacto(){
		if( jQuery('#cont').hasClass('figureclick')){
			jQuery('#cont').animate({right:'-35%'},"slow",function() {
				jQuery(this).removeClass('figureclick')});
		}else{
			jQuery('#cont').animate({right:'0px'},"slow",function() {
				jQuery(this).addClass('figureclick')});
		}
	}
  </script>
