<script type="text/javascript">
// -- Load Lazy Advertisement placement as deferred
try {
	$("div.lazyload_ad").lazyLoadAd({
	        threshold    : 500,         // You can set threshold on how close to the edge ad should come before it is loaded. Default is 0 (when it is visible).
	        forceLoad    : false,       // Ad is loaded even if not visible. Default is false.
	        onLoad       : false,       // Callback function on call ad loading
	        onComplete   : false,       // Callback function when load is loaded
	        timeout      : 1500,        // Timeout ad load
	        debug        : false,       // For debug use : draw colors border depends on load status
	        xray         : false        // For debug use : display a complete page view with ad placements
	}) ;
}
catch (exception) {
	console.log("Error with lazyLoadAd: "+exception);
}
</script>



<script type="text/javascript">
$(document).ready(function() {	
	// this is for the height of the main navigation	
	$(".wrapper nav.main").css('min-height',$(window).height() + 256);
 }); 

$(function () {
    var menu = $('#nav');
 	pos = menu.offset();

    $(window).scroll(function () {
        if ($(this).scrollTop() > pos.top + menu.height() && menu.hasClass('nav_default')) {
            menu.fadeOut('fast', function () {
                $(this).removeClass('nav_default').addClass('nav_fixed').fadeIn('fast');
                $('.wrapper').removeClass('default');
                $('.wrapper').addClass('fixed');
            });
        } else if ($(this).scrollTop() <= pos.top && menu.hasClass('nav_fixed')) {
            menu.fadeOut('fast', function () {
                $(this).removeClass('nav_fixed').addClass('nav_default').fadeIn('fast');
                $('.wrapper').addClass('default');
                $('.wrapper').removeClass('fixed');
            });
        }
    });
});


/* This is to place an overlay on Top Time Sucks images on hover. */
$('.top-frit ul li figure a img').after('<span class="item-overlay"> </span>');


// click the hamburger nav button
$('.wrapper .nav button').click(function(e){
    e.stopPropagation();
    $('.wrapper').toggleClass('nav-open');
});

 $('.close-btn').click(function(e){
	 $('.wrapper').removeClass('nav-open');
});

// this repositions the gpt-overlay and mobile welcome ad so that it's not covered by the header
$("div#superadunit").prependTo("div#page-wrapper");
//$("div#mobilewelcomead").prependTo("div#page-wrapper");


//this is to affix sticky ad on homepage and index pages (category, search, author)
var current_width = $(window).width();
//tablet or greater
if(current_width > 767){
	$(".right-rail.home > .ad").stick_in_parent({
		parent: '.homepage-wrapper',
		offset_top: 50,
		bottoming: false
	});
	
	$(".right-rail.index-pg > .ad").stick_in_parent({
		parent: '.bodee',
		offset_top: 50,
		bottoming: false
	});
	
	// removing ads after top frits module on tablets and desktop
	$( ".homepage-ad" ).remove();
}
if(current_width < 768) {
	$( ".right-rail.home" ).remove();
	$( "body.author section[role='main'] .bodee .right-rail.index-pg, .category section[role='main'] .bodee .right-rail.index-pg" ).remove();
}

$(document).ready(function() {
	// search input field
	var nameElement = document.forms.searchForm.query;
	
	function nameFocus( e ) {
	  var element = e.target || window.event.srcElement;
	  if ( element.value == "looking for somethin'?" )
	    element.value = "";
	}
	
	function nameBlur(e) {
	  var element = e.target || window.event.srcElement;
	  if ( element.value === "" )
	    element.value = "looking for somethin'?";
	}
	
	if ( nameElement.addEventListener ) {
	  nameElement.addEventListener("focus", nameFocus, false);
	  nameElement.addEventListener("blur", nameBlur, false);
	} else if ( nameElement.attachEvent ) {
	  nameElement.attachEvent("onfocus", nameFocus);
	  nameElement.attachEvent("onblur", nameBlur);
	}
});
</script>



	<script>
	//check to see if there's a fireplace loaded - if so, constrain the carousel width
	var setInt = setInterval(checkFireplace, 200);
	var setIntCount = 0;
	function checkFireplace() {
		//limit checking time to 6 seconds max
		if (setIntCount < 30) {
			if ($('body div').data('studioassetid')) {
				$('body').addClass('has-fireplace');	
				clearInterval(setInt);
			}
			
			setIntCount++;
		}
		else {
			clearInterval(setInt);	
		}
	}
	</script>





