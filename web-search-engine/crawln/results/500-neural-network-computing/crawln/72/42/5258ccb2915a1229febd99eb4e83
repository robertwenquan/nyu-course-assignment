<script src="/www/js/infinite-scroll.js"></script>
<script>
$(function() {
	var STREAM_CSS = ".item-stream";	// Selector for article stream container
	var FETCH_SIZE = 3;					// How many articles to fetch when end of page is reached

	var adCount = 0;
	// Initialize infinite-scroll
	IDG.InfiniteScroll.initialize({
		"callback" : function() {
			$.get("/napi/tile?def=most.popular")
			.done(function(data) {
				$(STREAM_CSS).append(data);
				// Insert ads on mobile after each infinite-scroll fetch
				if (IDG.IS_MOBILE) {
					var adDivId = 'homepage-ad-' + adCount++;
					$.get(
						"/napi/tile", {
							"def": "dynamic.ad.lazy.option",
							"lazyLoad" : false,
							"jquerySelector" : '#' + adDivId
						})
					.done(function(data) {
						$(STREAM_CSS).append('<div id="' + adDivId + '" class="homepage-ad">' + data + '</div>');
					});
				}
			});
		},
		"container" : STREAM_CSS,
		"ajax" : {
			"tile"  : "scrollItemList",
			"idKey" : "includedIds",
			"data" : {
				"includeArticles" : true,
				"pageType" : "homepage"
			}
		},
		"excludeIds" :	$(".homepage-stream-item").map(function() { return parseInt($(this).attr("data-articleid")) }).get(),
		"fetchSize" : FETCH_SIZE
	});
});
</script>

	
	


						
					
