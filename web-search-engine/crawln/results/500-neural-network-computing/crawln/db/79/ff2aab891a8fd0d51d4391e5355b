function trackGoogleAnalytics(articleId, slideNumber,articleUri) {
	_gaq.push(['_setAccount', 'UA-230061-1']);
	_gaq.push(['_setDomainName', 'networkworld.com']);
	//_gaq.push(['_trackPageview']);
	_gaq.push(['_trackPageview', articleUri +'/slideshow/' + slideNumber]);
	
}

