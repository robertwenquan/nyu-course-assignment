<!-- Script for Block 133:1005 -->
<script type='text/javascript'>


	$(document).ready(function() {
	    $('#validationDisplayBottom').hide(); 
            
         
	});

	$("#LoginRegister :input").not(":input[type=hidden], :input[type=submit]").each(function(){
		$(this).blur(function(){
			checkEmpty($(this));
			checkInvalid($(this));		
		});
	});

	function showMessage(element, type){
		$('#serverValidation').hide();
		element.parent().find('p[id*='+ type +']').attr('class', 'errorMessageInput');
		element.parent().attr('class', 'error');
		
	}

	function hideMessage(element, type){
                $('#serverValidation').hide();
		element.parent().find('p[id*='+ type +']').attr('class', 'errorMessageInput hidden');
		element.parent().attr('class', '');
		
	}

	function checkEmpty(element){
		if(!element.val() && element.attr('id')!="havePasswordInput") { 
		   showMessage(element, "blank");
		}
                else if(!element.val() && $('#havePassword').is(':checked') && element.attr('id')=="havePasswordInput"){
	            showMessage(element, "blank");
                }
		else if(element.val()) { 
		   hideMessage(element, "blank");
		} 
	}

	function validateEmail(email) {
                
		var filter = /^[a-z0-9.!#$%&'*+\/=?^_`{|}~-]+@[a-z0-9](?:[a-z0-9-]{0,61}[a-z0-9])?(?:\.[a-z0-9](?:[a-z0-9-]{0,61}[a-z0-9])?)*$/;
		return String(email.toLowerCase()).search (filter) != -1;
	}

    function validateHandle(handle) {
		var filter = /^(?:[a-zA-Z0-9]{6,30})?$/;
		return String(handle).search (filter) != -1;
	}

	function checkInvalid(element){
                
		if(element.val() && element.attr('id')=="EmailAddress" && !validateEmail(element.val())) { 
			showMessage(element,'invalid');
		} 
		else if(element.val() && element.attr('id')=="EmailAddress" && validateEmail(element.val())) { 
		    hideMessage(element,'invalid');
		} 

	}

	function addBlankMessage(text, element){
	 $("#validationDisplayBottom").css("display", "block");
	        $("#validationDisplayBottom").show();
	        if($("#validationDisplayBottom").text().indexOf(text) == -1){
	               $("#validationDisplayBottom").append('<li><span>'+ text + '</span></li>');
	        }
                $('#serverValidationDisplay').hide();
	        showMessage(element, "blank");
	}
	function addInvalidMessage(text, element){
		$("#validationDisplayBottom").css("display", "block");
	        $("#validationDisplayBottom").show();
	        if($("#validationDisplayBottom").text().indexOf(text) == -1){
	               $("#validationDisplayBottom").append('<li><span>'+ text + '</span></li>');
	        }
                $('#serverValidationDisplay').hide();
	        showMessage(element, "invalid");
	}
	function hidePasswordMessage(){
                var blankPasswordText = $('#havePasswordInput').parent().find('p[id*="blank"]').text();
                var passwordMessageToRemove = $('#validationDisplayBottom li:contains("'+blankPasswordText+'")');
                removeMessage($('#havePasswordInput'), passwordMessageToRemove, "blank");
                hideMessage($('#havePasswordInput'), "blank");
                if($('#EmailAddress').val() && validateEmail($('#EmailAddress').val())) { 
		      $('#validationDisplayBottom').hide();
		} 
                $('#serverValidationDisplay').hide();
                
        }
	function removeMessage(element, message, idType){
		message.remove();
		hideMessage(element, idType);
	}
	function checkFields(){
	   var areFieldsValidated = true;
	    $("#LoginRegister :input").not(":input[type=hidden], :input[type=submit]").each(function(){
		var blankText = $(this).parent().find('p[id*="blank"]').text();
		var invalidText = $(this).parent().find('p[id*="invalid"]').text();
	 
		if(!$(this).val() && $(this).attr('id')!="havePasswordInput") {     
			addBlankMessage(blankText, $(this));
			areFieldsValidated = false;
		}
        if(!$(this).val() && $('#havePassword').is(':checked') && $(this).attr('id')=="havePasswordInput"){
			addBlankMessage(blankText, $(this));
			areFieldsValidated = false;
		}
		if($(this).val() && $(this).attr('id') == 'EmailAddress' && !validateEmail($(this).val())) {     
			addInvalidMessage(invalidText, $(this));
			areFieldsValidated = false;
		}
		
	 
		
		var messageToRemove = $('#validationDisplayBottom li:contains("'+blankText+'")');
		var invalidMessageToRemove = $('#validationDisplayBottom li:contains("'+invalidText+'")');

		if($(this).val() && messageToRemove.text() == blankText){
			removeMessage($(this), messageToRemove, "blank");
		}  
		
		if($(this).val() && $(this).attr('id') == 'EmailAddress' && validateEmail($(this).val()) 
		   && invalidMessageToRemove.text() == invalidText){
			 
			removeMessage($(this), invalidMessageToRemove, "invalid");
		}
		    
		
	});
	if(areFieldsValidated){
	    $('#validationDisplayBottom').hide();
        }
	return areFieldsValidated;
}

</script>
<!-- end -->



