$(document).ready( function() {
    
    // Display privacy policy banner for 60 days on english sites
    var cookieName = "tt_privacy_policy_change";
    var bannerExpDateString = "Sat, 21 Mar 2015 05:00:00 GMT";
    var cookies = document.cookie;
    var privacyCookie;

    // Get privacy policy cookie if it exists
    var pos = cookies.indexOf(cookieName + '=');
    if (pos !== -1) {
        var start = pos + cookieName.length + 1;
        var end = cookies.indexOf(';', start);
        if (end === -1) {
            end = cookies.length;
        }
        privacyCookie = cookies.substring(start, end);
    } else {
        privacyCookie = '';
    }
    
    // If it's been 60 days, delete the cookie and don't show anything.
    var now = new Date();
    var bannerExpDate = new Date(bannerExpDateString);
    if (now > bannerExpDate) {
        if (privacyCookie) {
            document.cookie = cookieName + '=; expires=Thu, 01 Jan 1970 00:00:01 GMT;';
        }       
        return;
    }
    if (privacyCookie) {
        // User should only see the banner once. Keep cookie until 60 days have past.
        return;
    }
    var lang = $('html').attr('lang') || 'en';
    if (lang != "en") {
        // Only display on english sites.
        return;
    }

    var messageHtml = '<div class="header-privacy-policy">' +
        '<span class="pp-strong">Our Privacy Policy has changed.</span>' + 
        '<span>By continuing to use this site, you are agreeing to the new ' +
        '<a href="http://www.techtarget.com/html/privacy_policy.html" target="_blank">' +
        'Privacy Policy</a>.</span></div>';
    $('#header').prepend(messageHtml);
    
    var result = /(?:(?:dev\.eng|eng|qa)\.)?[^\.]+(?:\.|\.co\.|\.com\.)[^\.]+$/.exec(window.location.hostname);
    var domain = result ? result[0] : "";
    if (domain) {
        domain = domain.replace(/(dev\.eng|eng|qa)\./i, "");
    }
    document.cookie = cookieName + "=true; expires=" + bannerExpDateString + "; path=/; domain=." + domain + ";";
    
});
</script>

