/*search checkbox submit function*/
$(function() {
	$('.search_docs input:checkbox').click(function(){
		var check_class=$(this).parents('.search_result_content').attr('id');
                var check_count= $('#'+check_class).attr('data-attr');
                if(check_count=='one'){
                  if($('#'+check_class).find('input[type="checkbox"]:checked').length>1){
                    $('#'+check_class).find('input[type="checkbox"]').removeAttr('checked');         
                    $('#'+check_class).find(this).attr('checked','checked');
                  }
                }
	});

	$('#search_doc_submit').click(function(){
                var wpl_tax = ""
                $('#menu_wpl input[type="checkbox"]:checked').each(function(){
                  wpl_tax+=$(this).attr("value")+"$";
                });
                var multiple_taxonomy = $('.search_docs #wpl_taxonomy_names').attr('value');
                multiple_taxonomy+= wpl_tax
                $('.search_docs #wpl_taxonomy_names').attr('value',multiple_taxonomy) 
	//	$('.search_docs #wpl_taxonomy_names').attr('value',$('#menu_wpl input[type="checkbox"]:checked').attr('value'))
		$('.search_docs #content_type').attr('value',$('#menu_type input[type="checkbox"]:checked').attr('value'))
		$('.search_docs #month_year_based').attr('value',$('#menu_date input[type="checkbox"]:checked').attr('value'))
		$('.search_docs #company_name').attr('value',$('#menu_company input[type="checkbox"]:checked').attr('value'));
                $('.search_docs form input').each(function(){
                        if($(this).attr('value')==""){
                                $(this).removeAttr('name')
                        }
                });
                $('.relevance_select').removeAttr('name');
		$('.search_docs #search_form').submit();
	});


         $('.search_author input:checkbox').click(function(){
	   var check_class=$(this).parents('.search_result_content').attr('id');
           var check_count= $('#'+check_class).attr('data-attr');
           if(check_count=='one'){
             if($('#'+check_class).find('input[type="checkbox"]:checked').length>1){
               $('#'+check_class).find('input[type="checkbox"]').removeAttr('checked');		
               $('#'+check_class).find(this).attr('checked','checked');
        	}
         	}  
	});
 
	$('.search_author #search_submit').click(function(){
                var wpl_tax = ""
                $('#menu_cat input[type="checkbox"]:checked').each(function(){
                  wpl_tax+=$(this).attr("value")+"$";
                });
                var multiple_taxonomy = $('.search_author #wpl_taxonomy_names').attr('value');
                multiple_taxonomy+= wpl_tax
                $('.search_author #wpl_taxonomy_names').attr('value',multiple_taxonomy)
	//	$('.search_author #wpl_taxonomy_names').attr('value',$('#menu_cat input[type="checkbox"]:checked').attr('value'))
		$('.search_author #section').attr('value',$('#menu_content input[type="checkbox"]:checked').attr('value'))
		$('.search_author #month_year_based').attr('value',$('#menu_publish input[type="checkbox"]:checked').attr('value'))
		$('.search_author #author').attr('value',$('#menu_comp input[type="checkbox"]:checked').attr('value'));
                $('.search_author form input').each(function(){
			if($(this).attr('value')==""){
				$(this).removeAttr('name')
			}
		});
                $('.relevance_select').removeAttr('name');
		$('.search_author #search_form').submit();
	});
});
</script>

<script type="text/javascript">
$(function(){

 $('.cancel_search').click(function(){
 var author = $("#author").val();
 if(author==undefined){
 var serch_loc=location.protocol+"//"+location.host+"/full_library";
 }
 else{ 
 var serch_loc=location.protocol+"//"+location.host+"/search?utf8=%E2%9C%93&amp;query=&amp;facet=true&amp;sort=Date";
 }
 window.location.href=serch_loc;
 });

$(".search_close").click(function(){
var block_id= '#'+$(this).parents('.search_result').attr('id');
var check_data = $(this).attr('data-id')
if(check_data=="wpl_taxonomy_names"){
  var taxon = ""
  $(block_id+" .search_close[data-id='wpl_taxonomy_names']").not($(this)).each(function(){
    taxon+=$(this).attr("data-value")+"$";
  });
  if(taxon==""){
    $(block_id).find('#'+$(this).attr('data-id')).attr('name','');
  }
  else{
    $(block_id).find('#'+$(this).attr('data-id')).attr('value',taxon);
  }
}
else{
$(block_id).find('#'+$(this).attr('data-id')).attr('name','');
}
$(this).parent().remove();
$(block_id).find('form input').each(function(){
    if($(this).attr('value')==""){
        $(this).removeAttr('name')
     }
});
   $('.relevance_select').removeAttr('name');
   $(block_id).find('form').submit()
});

});

