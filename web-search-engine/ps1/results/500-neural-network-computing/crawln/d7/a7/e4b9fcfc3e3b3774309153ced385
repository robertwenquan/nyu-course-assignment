<script type="text/javascript">
	jQuery(document).ready(function () {
	  var device_used;
          var entity_id=$('#entity_id').val();
          var entity_type=$('#entity_type').val();
	  var isiPad = /ipad/i.test(navigator.userAgent.toLowerCase());
  //    $.get("http://ipinfo.io", function(response) {
        $.browser.device = (/android|webos|iphone|ipad|ipod|blackberry|iemobile|opera mini/i.test(navigator.userAgent.toLowerCase()));
       if($.browser.device)
		{
			mobile = ['iphone','ipad','android','blackberry','nokia','opera mini','windows mobile','windows phone','iemobile']; 
            for (var i in mobile) 
             {
            	if (navigator.userAgent.toLowerCase().indexOf(mobile[i].toLowerCase()) > 0) {
            		device_used = mobile[i].toLowerCase() ;
            	}
            }
        }
		else
		{
		   device_used = 'Desktop'
		}
	    $.ajax({
			url : "/users/behavorial_tracking?browser_name="+$.browser.name+"&resolution="+$(document).width()+"*"+$(document).height()+"&device_used="+device_used+"&is_ipad="+isiPad+"&url="+$(location).attr('href')+"&ip="+ "" +"&entity_id="+entity_id+"&entity_type="+entity_type,
			type : 'POST',
			dataType:'json',
			beforeSend : function(xhr) {
				xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'));
			},
			success : function(msg) {
				    //alert($.browser.name + ", " + $(document).width()+"*"+$(document).height() + ', '+ device_used + ', ' + isiPad);
			},
        });	
//	  }, "jsonp");
   });
</script>
		</body>
</html>
