<script type="text/javascript">
$(function(){
	newsTicker()
//updateTicker();
 setInterval("updateTicker()",1800000)
});

var updateTicker = function(){
        $('#newsticker_block').empty()
	$.ajax({
		url :'/comment_ticker',
		type:'POST',
		dataType:'html',
                beforeSend: function(xhr){
                xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))
                },
		success:function(ticker){
			//$('#news_ticker').empty();
			//$('#news_ticker').html(ticker);
                         $('#newsticker_block').html(ticker);
                        newsTicker();
		}
	});
}

var newsTicker = function(){
	$('#news_ticker').vTicker({ 
		speed: 700,
		pause: 3000,
		animation: 'fade',
		mousePause: false,
		showItems: 3,
		padding:4,
		height:400
	});
}
</script>
<script type="text/javascript">
		$(function() {
           $(".page_display_bar_right").on("change","select",function(){
           $(".page_display_bar_right span.select_overlay").html($("option:selected",this).text())
            });
            }); 
	</script>

<script type="text/javascript">
$(function(){
	$('#container_left_image_parent_slide .container_left_image:first').addClass('active')
        var left = 0;
$('#container_left_image_parent_slide .container_left_image').each(function(){
    $(this).attr('style','left:'+left+'%')
    left = left+100;
});
});
		$('.container_top').attr('data-value','1,2,3,4')
            var slideIndex=1;
		 	var looping_function = function(){
				var arr = $('.container_top').attr('data-value').split(',')
				//console.log(arr)
				var arr1 = [];
				arr1[0]=arr[0],
				arr1[1]=arr[1],
				arr1[2]=arr[2],
				arr1[3]=arr[3]
			
				arr[0] = arr[arr.length-1],
				arr[1] = arr1[0],
				arr[2] = arr1[1],
				arr[3] = arr1[2];
				$('.container_top').attr('data-value',arr)
                
                if(slideIndex==arr.length){
                    left="0%";slideIndex=1;duration=0;
                }else{
                    left="-"+((slideIndex)*100)+"%";slideIndex+=1;duration=1000
                }
                
                $("#container_left_image_parent_slide").animate({
                    left: left
                }, {
                    duration: duration,
                    specialEasing: {
                      height: "easeOutBounce"
                    },
                    start:function(){
                        $("#container_left_image_parent_slide .container_left_image span").animate({
                        bottom:"-116px"
                      },500)
                      $("#container_left_image_parent_slide .container_left_image .hover_image_1").animate({
                        bottom:"-116px"
                      },500)
                    },
                    complete: function() {
                      $("#container_left_image_parent_slide .container_left_image").removeClass("active")
                      $("#container_left_image_parent_slide .container_left_image:nth-child("+slideIndex+")").addClass("active")
                      $("#container_left_image_parent_slide .container_left_image:nth-child("+slideIndex+") span").animate({
                        bottom:"0px"
                      },500)
                      $("#container_left_image_parent_slide .container_left_image:nth-child("+slideIndex+") .hover_image_1").animate({
                        bottom:"0px"
                      },500)
                    }
                  });
                
               
				right_image_details=image_details.filter(function (item,index) { return index !== (slideIndex-1) });
			    $.each(right_image_details,function(index,val){
			      $('.container_right_image .right_image_1:eq("'+index+'")>a').attr('href',val['image_anchor'])
			      $('.container_right_image .right_image_1:eq("'+index+'")>a>img').attr('src',val['image_anchor_small_image_src'])
			      $('.container_right_image .right_image_1:eq("'+index+'") div.hover_image_2.left_content p>a').attr('href',val['content_block_para_anchor_href']);
			      $('.container_right_image .right_image_1:eq("'+index+'") div.hover_image_2.left_content p>a').text(val['content_block_para_anchor_text'])
			      $('.container_right_image .right_image_1:eq("'+index+'") div.hover_image_2.left_content h2>a').attr('href',val['content_block_h2_anchor_href']);
			      $('.container_right_image .right_image_1:eq("'+index+'") div.hover_image_2.left_content h2>a').text(val['content_block_h2_anchor_text'])
			      $('.container_right_image .right_image_1:eq("'+index+'") div.hover_image_2.left_content ul li.image_comment>a').attr('href',val['content_block_ul_imagecomment_anchor_href']);
			      $('.container_right_image .right_image_1:eq("'+index+'") div.hover_image_2.left_content ul li.image_comment>a').text(val['content_block_ul_imagecomment_anchor_text']);
			      $('.container_right_image .right_image_1:eq("'+index+'") div.hover_image_2.left_content ul li:eq(1)>a').text(val['content_block_ul_imagecomment_anchor_date'])
			    });

			}
			var refreshId = setInterval("looping_function()",8000)
			$('.container_top').mouseover(function(){
				clearInterval(refreshId);
			});
			$('.container_top').mouseout(function(){
				refreshId = setInterval("looping_function()",8000)
			});
		
	</script>
			
			<script type="text/javascript">
    $(document).ready(function(){
        jQuery.ajax({
            type: 'GET',
            dataType: 'html',
            beforeSend: function(xhr){
                xhr.setRequestHeader('X-CSRF-Token', $('meta[name="csrf-token"]').attr('content'))
            },
            url: '/verify-site-login',
            success: function(update_data){
                jQuery(".log_register").html(update_data);
            }
        });
    });
    
</script>


<script type="text/javascript">
			$(document).ready(function(){
				$(window).load(function() {
				  // The slider being synced must be initialized first
				  $('#carousel1').flexslider({
				    animation:"slide",
				    controlNav: false,
				    animationLoop: false,
				    //slideshow: false,\
				    itemWidth: 144,
				    itemMargin: 5,
				    asNavFor: '#slider1'
				  });
				   
				  $('#slider1').flexslider({
				  	keyboardNav: true,
				    animation: "slide",
				    controlNav: false,
				    animationLoop:true,
				    slideshow:true,
				    pausePlay:true,
				    pauseText: 'PAUSE SLIDES',
				    playText: 'PLAY SLIDES', 
				    slideshowSpeed: 7000,
				    
				    controlsContainer: ".flex-container",
				    //sync: "#carousel1",
				    start: function(slider) {
			            slider.find('.slideIndex').html(slider.currentSlide + 1 +  " / "  + slider.count);
			            $('a.first-slide').click(function() {
				            $('.flexslider').show();
				            var slideTo = $(this).attr("rel");
				            var slideToInt = parseInt(slideTo);
				            if (slider.currentSlide != slideToInt) {
				                slider.flexAnimate(slideToInt);
				            }
				        });
				        $('a.last-slide').click(function() {
				            $('.flexslider').show();
				            var slideTo = $(this).attr("rel");
				            var slideToInt = parseInt(slideTo);
				            if (slider.currentSlide != slideToInt) {
				                slider.flexAnimate(slideToInt);
				            }
				        });
				        $(".previous-slide").click(function(){
							$('#slider1').flexslider('previous');
						});
						$(".next-slide").click(function(){
							$('#slider1').flexslider('next');
						});
			          },
			        after: function(slider) {
			        	
				        slider.find('.slideIndex').html(slider.currentSlide + 1 +  "/"  + slider.count);
				        
				    }
				  });
				});
			});
			
		</script>
		
		
		<script type="text/javascript">
		$(document).ready(function(){
				$(".fullscreen").click(function(){
				      //$(".ad-image-wrapper").fullScreen(false);    
				    if (screenfull.enabled) {
				        screenfull.toggle($('#slide-gal')[0]);
				    }else{
				      if($('.slide-play-block').hasClass('fullgallery')){
				    	esc_fullscreen()
				      }else{
				    	$(document).scrollTop(0);
				    	$('.slide-play-block').addClass("fullgallery");
				    	$('.over_lay').css('display','block');
				    	$('body').css('overflow','hidden');
				      }
				        return false;
				    }
				 });
			
				  var esc_fullscreen = function(){
				  		$(document).scrollTop(100);
				        $('.slide-play-block').removeClass("fullgallery");
				        $('.over_lay').css('display','none');
				        $('body').css('overflow','scroll')
				  }
			
				$(document).keyup(function(e) {
				 	if (e.keyCode == 27) {
				 		esc_fullscreen();
				 	}  
				});
			
				$(document).on('click','.over_lay',function(){
				    esc_fullscreen();
				});
				

});
</script>



