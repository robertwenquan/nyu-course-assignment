		<script type="text/javascript">
			var isAuthenticated = false;
			var followedIds = new Array;

			$(document).ready(function(){
				$.ajax({
					type:'POST',
					url:'/question/status',
					success:function(data){
						if(data && data.readOnly){
							$('ul.user').hide();
							$('.postBody').hide();
							$('input[type=submit]').hide();
							$('#search-form a').removeAttr('href');
							$('#notAuthQstOpt .buttons').hide();
							
							if($.cookie('memberId')){
								signout();
							}
						}
					}
				});
			});
		</script>


	
