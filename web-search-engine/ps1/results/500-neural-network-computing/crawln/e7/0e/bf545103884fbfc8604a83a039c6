	                    		<script>
	                    			function onBeforeScreenLoadHandler(eventObj) {
	                    				if ($('.gigya-screen-dialog-mobile').length < 1 && $('.gigya-bg-overlay').length < 1) {
	                    					$('body').append('<div class="gigya-bg-overlay"></div>');
                    						$('.gigya-bg-overlay').height($(document).height());
                    						var activeScreenSet = $(this)[0].screenSet;
                    						$(document).on("click", ".gigya-bg-overlay", function(e){
                    							e.preventDefault();
                    							gigya.accounts.hideScreenSet({ screenSet: activeScreenSet });
                    						});
                    					}
	                    			}
	                    		   
	                    		   function onAfterScreenLoadHandler(eventObj){	                    					
	                   					var containerID = $(this)[0].containerID;
	                   					if( $(".gigya-screen-dialog-close").length < 1 ) {
	                   						$(".gigya-screen-dialog-mobile").prepend('<div class="gigya-screen-dialog-close" id="' + containerID + '_content_close"></div>');	                   						
	                   					}
	                   					var activeScreenSet = $(this)[0].screenSet;
	                   					$(document).on("click", ".gigya-screen-dialog-close", function(e){
                   							e.preventDefault();
                   							gigya.accounts.hideScreenSet({ screenSet: activeScreenSet });
                   						});
	                   				}
	                    		   
	                    		   function onHideHandler(eventObj) {
	                   					$(".gigya-bg-overlay").remove();
	                   				}
	                    		</script>
	                    		<script src="https://cdns.gigya.com/js/socialize.js?apiKey=3_5TevHKhLtyFeMp1CydakxENdnNxO4ugohr0oiNDaZ5bfbD5Y_L6nLo7CFSAg4u27">
	                    		{
	                    			onBeforeScreenLoad: onBeforeScreenLoadHandler,
                    				onAfterScreenLoad:onAfterScreenLoadHandler,
                    				onHide: onHideHandler
	                    		}
	                    		</script>
	                    		<script>
	                    			signinParams = {
	                    				screenSet:'Login-web-captcha'	                    				
	                    			}
	                    			
	                    			registerParams = {
                    					screenSet:'Login-web-captcha', 
                    					startScreen:'gigya-register-screen'
	                    			}
	                    		</script>
	                    		<li class="signed-out" id="signed-out" style="display:none;"><a href="javascript://" onclick="gigya.accounts.showScreenSet(signinParams);">Sign in</a> | <a href="javascript://" id="lnkHeaderRegister" onclick="gigya.accounts.showScreenSet(registerParams);">Register</a></li>
	                    		<script type='text/javascript'>
	                    		//function onLoad() {
						            // register for login event
	            					
	            					gigya.accounts.addEventHandlers({
		                    			 onLogin: onLoginHandler
		                    			});
	 
	        					//}
	                    		function onLoginHandler(eventObj) { 
	                    			console.log(eventObj);
	                    			gigya.accounts.hideScreenSet(registerParams);
	                    			gigya.accounts.hideScreenSet(signinParams);
	                    			//alert('login sig' + eventObj.signature + " ts " + eventObj.timestamp);
	                    			$.ajax('/user/gigyaLoginAction',{
	                    				type: "POST",
	                    				data:{"signatureTimestamp":eventObj.timestamp,"signature":eventObj.signature,"gUid":eventObj.UID},
	                    				success: function(data){
	                    					isAuthenticated = true;
	                    					$("#login-result").replaceWith(data);
	                    					
	                    					if ($("#notAuthQstOpt").length>0) {
	                    						//logged in from stack template, enable auth question options
	                    						$("#notAuthQstOpt").hide();
	                    	                	$("#authQstOpt").show();
	                    					}
	                    					
	                    					if ($("#askModule").length>0) {
												//display ask module
	                    	                	$("#askModule").show();
	                    					}
	                    					
	                    				},
	                    				error: function(jqXHR,error,thrown){
	                    					alert("Unable to log in right now, please try again later");
	                    				}
	                    				
	                    			});
	                    			
	                    			
	                    		}
	                    		
	                    		function onFieldChangedHandler(eventObj) { 
	                    			
	                    			//alert('login sig' + eventObj.signature + " ts " + eventObj.timestamp);
	                    			$.ajax('/user/gigyaProfileUpdateAction',{
	                    				type: "POST",
	                    				data:{"signatureTimestamp":eventObj.timestamp,"signature":eventObj.signature,"gUid":eventObj.UID, "mid": $.cookie("memberId")},
	                    				success: function(data){
	                    					isAuthenticated = true;
	                    					$("#login-result").replaceWith(data);
	                    				},
	                    				error: function(jqXHR,error,thrown){
	                    					alert("Unable to log in right now, please try again later");
	                    				}
	                    				
	                    			});
	                    		}
	                    		
	                    		function signout(){
	                    			isAuthenticated = false;
	                    			gigya.accounts.logout();
	                    			followedIds = new Array;
	                    			$.cookie("member",null , {expires: -1, path: '/'}); //session cookie
	                    			$.cookie("memberId", null, {expires: -1, path: '/'});
	                    			$.removeCookie("member", { path: '/' });
	                    			$.removeCookie("memberId", { path: '/' });
	                    			$.removeCookie("memberPic", { path: '/' });
	                    			$.removeCookie("followedIds", { path: '/' });
                    				document.getElementById('signed-in').style.display="none";
                    				document.getElementById('signed-out').style.display="block";
                    				document.getElementById('loginuser-img').src = "http://idge.staticworld.net/ans/default-user.png";       
                    				$('.header-site-message').addClass('hidden');// set to "removeClass" to get the message to show up again
	                    		}
	                    		
	                    		function ask(){
	                    			if (isAuthenticated){
	                    				window.location = "/question/add";
	                    			}
	                    			else {
		           						 gigya.accounts.showScreenSet({screenSet:'Login-web-captcha'});                  				
	                    			}
	                    		}
	                    		
	                    		</script>
	                    		<div id="login-result"></div>
	                    		<li class="signed-in" id="signed-in" style="display:none;">
	                    			<div class="user-info">
	                    				<!-- if there's no user icon, default to the default-user.png image below -->
	                    				<div id="loginuser-img"><img src="http://idge.staticworld.net/ans/default-user.png" class="avatar user-img" /></div>
	                    				<span class="user-name"><a id="membername" href="">username</a> <i class="ss-icon ss-dropdown"></i></span>
	                    				<ul class="user-actions">
	                    					
	                    						<li><a href="/question/profile">My Profile</a></li>
	                    					
	                    					<li><a href="" onClick="signout();">Sign Out</a></li>
	                    				</ul>
	                    			</div>
	                    		</li>
	                    		<script type='text/javascript'>
	                    		$(document).ready(function(){
	                    			var memberId = $.cookie('memberId');
	                    			var memberName= $.cookie("member");
	                    			var memberPic= $.cookie("memberPic");
	                    			if (memberId && memberName){
	                    				isAuthenticated = true;
	                    				document.getElementById('membername').innerHTML = memberName;
	                    				document.getElementById('signed-in').style.display="block";
	                    				document.getElementById('signed-out').style.display="none";
	                    				document.getElementById('search-intro').style.display="none";
	                    				$('.header-site-message').addClass('hidden');// set to "removeClass" to get the message to show up again
	                    				
	                    				var cookieFollowedIds= $.cookie("followedIds");
	                    				if (cookieFollowedIds){
	                    					followedIds = JSON.parse(cookieFollowedIds);
	                    				} 	                    				
	                    				
	                    				if (memberPic){
		                    				// document.getElementById('loginuser-img').src = memberPic;
		                    				document.getElementById('loginuser-img').innerHTML =  '<img src="' + memberPic + '" class="avatar user-img" />';
		                    			} else {
		                    				document.getElementById('loginuser-img').innerHTML = '<div class="avatar default-avatar"><div class="initial">' + memberName.charAt(0) + '</div></div>';
		                    			}
	                    				
	                    			} else {
	                    				document.getElementById('signed-in').style.display="none";
	                    				document.getElementById('signed-out').style.display="block";
	                    				$('.header-site-message').addClass('hidden');// set to "removeClass" to get the message to show up again
	                    			}
	                    			
	                    			if (location.search.substring(8)==1) {
	                    				gigya.accounts.showScreenSet(signinParams);
	                    			}
	                    		});
