<script type="text/javascript" src="//www.idc.com/js/autocomplete.js"></script>
<script>
    function sendSearch() {
        var selectedLanguage = $('#selectedLanguage').text();
        if(selectedLanguage == 'Any language'){
            selectedLanguage = "";
        }
        $(".searchbar input[type=hidden]").val(selectedLanguage);
        document.basicSearchForm.submit();
    }

    // Search Bar dropdown language selection
    function DropDown(el) {
        this.dd = el;
        this.placeholder = this.dd.children('span');
        this.opts = this.dd.find('ul.language > li');
        this.val = '';
        this.index = -1;
        this.initEvents();
    }
    DropDown.prototype = {
        initEvents : function() {
            var obj = this;

            obj.dd.on('click', function(event){
                $(this).toggleClass('active');
                return false;
            });

            obj.opts.on('click',function(){
                var opt = $(this);
                obj.val = opt.text();
                obj.index = opt.index();
                obj.placeholder.text(obj.val);
            });
        },
        getValue : function() {
            return this.val;
        },
        getIndex : function() {
            return this.index;
        }
    }

    $(function() {

        var dd = new DropDown( $('.styled-select') );

        $(document).click(function() {
            // all dropdowns
            $('.styled-select').removeClass('active');
        });

    });
</script>
