<script type="text/javascript" >
    $(document).ready(function() {

        $("#loggedUser").click(function() {
            var X=$(this).attr('id');
            if(X==1)
            {
                $(".loginInfo").hide();
                $(this).attr('id', '0');
            }
            else
            {
                $(".loginInfo").show();
                $(this).attr('id', '1');
            }
        });

        //Mouse click on sub menu
        $(".loginInfo").mouseup(function() {
            return false
        });

        //Mouse click on my account link
        $("#loggedUser").mouseup(function() {
            return false
        });


        //Document Click
        $('body').mouseup(function() {
            $(".loginInfo").hide();
            $("#loggedUser").attr('id', '');
        });

        //Document Click for iPad
        $(document).bind( 'touchstart', function(e){
            $(".loginInfo").hide();
            $("#loggedUser").attr('id', '');
        });
        $('.loginInfo').on('touchstart', function(e){
            e.stopPropagation();
        });

    });

    function showSub(num) {
        $("#subSubMenu" + num).removeClass('submenu-manageAccount_off');
        $("#subSubMenu" + num).addClass('submenu-manageAccount');
    }

    function hideSub(num) {
        $("#subSubMenu" + num).removeClass('submenu-manageAccount');
        $("#subSubMenu" + num).addClass('submenu-manageAccount_off');
    }
</script>
