<script type="text/javascript" >
$(document).ready(function() {

  $(".ls-link").click(function() {
    var X=$(this).attr('id');
    if(X==1)
    {
      $("#localsites").hide();
      $(this).attr('id', '0'); 
    }
    else
    {
      $("#localsites").show();
      $(this).attr('id', '1');
    }
  });

  //Mouse click on sub menu
  $("#localsites").mouseup(function() {
    return false
  });

  //Mouse click on my account link
  $(".ls-link").mouseup(function() {
    return false
  });


  //Document Click
  $(document).mouseup(function() {
    $("#localsites").hide();
    $(".ls-link").attr('id', '');
  });
  
  //Document Click for iPad
  $(document).bind( 'touchstart', function(e){
    $("#localsites").hide();
    $(".ls-link").attr('id', '');
  });
  $('#localsites').on('touchstart', function(e){ 
    e.stopPropagation();
  });
  
}); 
</script> 
