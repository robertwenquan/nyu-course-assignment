<script>
function isBlockedEmail(str) {
var blocked = ['@hotmail.com', '@yahoo.com', '@msn.com', '@me.com', '@live.com', '@aol.com', '@org.com', '@123.com', '@kaseya.com'];
for (var i = 0; i < blocked.length; i++) {
if (str.indexOf(blocked[i]) != -1) {
return false;
}
}
return true;
}
function validatePhone(phone) {
if (phone.length >= 7 && phone.length <= 21) {
return true
} else {
return false
}
}
MktoForms2.loadForm("//app-sj05.marketo.com", "596-INX-704", 1305, function (form) {
//Set values for the hidden fields, "LeadStatus" and "MiddleName".
//Note that these fields were configured in the form editor as hidden fields already.
//listen for the validate event
form.onValidate(function () {
//get the values
var vals = form.vals();
// get email value and check if it's blocked
var emailIsBlocked = isBlockedEmail(vals.Email);
var phoneIsBlocked = validatePhone(vals.Phone);
//Check email condition
if (emailIsBlocked == false) {
//prevent form submission
form.submittable(false);
//Show error message, pointed at email element
var emailElem = form.getFormElem().find("#Email");
form.showErrorMessage("Please use a valid enterprise email address (not hotmail, yahoo, etc.)", emailElem);
} else if (phoneIsBlocked == false) { //Check phone condition
//prevent form submission
form.submittable(false);
//Show error message, pointed at phone element
var phoneElem = form.getFormElem().find("#Phone");
form.showErrorMessage("The phone field must contain at least 7 and no more then 21 characters!", phoneElem);
} else {
//enable submission for those who met the criteria.
form.submittable(true);
}
});
});
</script>
