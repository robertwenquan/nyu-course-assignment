	<script>
	//check to see if there's a fireplace loaded - if so, constrain the carousel width
	var setInt = setInterval(checkFireplace, 200);
	var setIntCount = 0;
	function checkFireplace() {
		//limit checking time to 10 seconds max
		if (setIntCount < 50) {
			if ($('body div').data('studioassetid')) {
				$('body').addClass('has-fireplace');	
				clearInterval(setInt);
			}
			
			setIntCount++;
		}
		else {
			clearInterval(setInt);	
		}
	}
	</script>


