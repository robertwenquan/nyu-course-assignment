      function ozCssLoaded() {
        if (window.jstiming) {
          window.jstiming.load.tick('xcr');
      
          if (window['onCssLoaded']) {
            window['onCssLoaded']();
          } else {
            window['onCssLoaded'] = true;
          }
        }
      }

