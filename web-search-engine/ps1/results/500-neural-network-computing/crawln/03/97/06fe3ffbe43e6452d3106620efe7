_gaq.push(['_setAccount', 'UA-300704-1']);
_gaq.push(['_setDomainName', 'computerworld.com']);

if(""=="11"){
	 _gaq.push(['_trackPageview',''+'/slideshow/'+ '1']);
} else{
 _gaq.push(['_trackPageview']);
}
(function() { 
	var ga = document.createElement('script');
	ga.type = 'text/javascript';
	ga.async = true;
	ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
	var s = document.getElementsByTagName('script')[0];
	s.parentNode.insertBefore(ga, s); }
)();

function trackGoogleAnalytics(articleId, slideNumber,articleUri) {
	_gaq.push(['_setAccount', 'UA-300704-1']);
	_gaq.push(['_setDomainName', 'computerworld.com']);
	//_gaq.push(['_trackPageview']);
	_gaq.push(['_trackPageview', articleUri +'/slideshow/' + slideNumber]);
	
}

</script>




















