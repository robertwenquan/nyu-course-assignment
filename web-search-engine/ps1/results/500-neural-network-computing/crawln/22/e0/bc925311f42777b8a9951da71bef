IDG.video.players['bcplayer-idgtv'].vjs.ready(function() {
	
	
	$thm.logPlEvent({"b":"ctw","e":"view","t":"media","id":"56385"});

	var thisPlayer = IDG.video.players['bcplayer-idgtv'];		
	
	
	
	
				
	
	

				
	
	
	this.on("play", function() {
				
		$thm.logPlEvent({"b":"ctw","e":"play","t":"media","id":"56385"});
		if (window.PARSELY) {
            PARSELY.videoPlaying = true;
            //console.log("PARSELY.videoPlaying = true");
        }
        
        
					
		
	});
	this.on("pause", function() { 
		if (window.PARSELY) {
            PARSELY.videoPlaying = false;
            //console.log("PARSELY.videoPlaying = false");
        }
	});	
	
	var adCountDown;
	this.on("adstart", function() {
		IDG.GPT.backfillCompanionIfEmpty('gpt-skin', $('#skinAdTarget'));
		if (IDG.video.players['bcplayer-idgtv'].vjs.ima3.adsManager) {
			var $thisVideo = $(this.el());
			var $thisVideoParent = $thisVideo.parent();
			var $thisVideoItemText = $thisVideoParent.find('.item-text');
			var $thisVideoOverlay = $thisVideoParent.find('.over');
			$thisVideo.removeClass('not-loaded');
			$thisVideoItemText.remove();
			$thisVideoOverlay.remove();		
			$('#bcplayer-idgtv').append('<div id="bcplayer-idgtvCountdown" class="preroll-countdown hidden">Your video begins in <span class="countdown">' + Math.round(IDG.video.players['bcplayer-idgtv'].vjs.ima3.adsManager.getRemainingTime()) + ' seconds.</span></div>');
			adCountDown = setInterval(function() {
			  var timeRemaining = IDG.video.players['bcplayer-idgtv'].vjs.ima3.adsManager.getRemainingTime();
			  var timeRemainingRounded = Math.round(timeRemaining);			 
			  if (Math.round(timeRemaining) > 0 && $("#bcplayer-idgtvCountdown").hasClass('hidden')) {
			  		$("#bcplayer-idgtvCountdown").removeClass('hidden')
			  } 
			  var timeUnit;
			  if (timeRemainingRounded == 1) {
				  timeUnit = 'second';
			  }
			  else {
				  timeUnit = 'seconds';
			  }
			  	$("#bcplayer-idgtvCountdown .countdown").html(Math.round(timeRemaining) + ' ' + timeUnit + '.');
			  }, 1000);
		}	
	});
	this.on("adserror", function() { 
		IDG.GPT.backfillCompanionIfEmpty('gpt-skin', $('#skinAdTarget')); 
	});
	this.on("adend", function(){
		if (adCountDown) {
			clearInterval(adCountDown);
		}
		$("#bcplayer-idgtvCountdown").remove();
	});
	
	
	
	
});
	

	  		IDG.video.players['bcplayer-idgtv'].vjs.on('ended', IDG.video.showSimilar);
	  		

