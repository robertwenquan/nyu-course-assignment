	$(document).ready(function(){
		var toasterHoverTimeout;
		var toasterHoverOutTimeout;
		
		$('.toaster').hover(function(){
			clearTimeout(toasterHoverOutTimeout);
			toasterHoverTimeout = setTimeout(function(){$('.toaster .tab').hide();$('.toaster .show-next').show();},250);			
		}, function(e) {
			clearTimeout(toasterHoverTimeout);
			toasterHoverOutTimeout = setTimeout(function(){$('.toaster .show-next').hide();$('.toaster .tab').show();},500);
		});
		
		//keep this for touch devices and for clicking to re-open after clicking the close icon
		$('.toaster .tab').click(function(e){
			e.preventDefault();
			$('.toaster .tab').hide();
			$('.toaster .show-next').show();
			clearTimeout(toasterHoverTimeout);
			clearTimeout(toasterHoverOutTimeout);
		});
		
		$('.toaster .show-next .ss-delete').click(function(e){
			e.preventDefault();
			$('.toaster .show-next').hide();
			$('.toaster .tab').show();
			clearTimeout(toasterHoverOutTimeout);
		});
	});


	$(document).ready(function(){
		$('.open-this').click(function(e){
			e.preventDefault();
			e.stopPropagation();			
			var target=$(this).attr('href');
			$('.default-hide.open:not(' + target +')').removeClass('open');
			$(target).addClass('open');
			if ($(target).hasClass('open')) {
				//give the other divs a chance to collapse
				setTimeout(function(){var targetTop = $(target).offset().top;$('html,body').animate({scrollTop: ($(target).offset().top)-105}, 500);},150);				
			}
		});
		
		$('.close-this').click(function(e){
			e.preventDefault();
			e.stopPropagation();			
			var target=$(this).attr('href');
			$(target).removeClass('open');
			var targetTop = $(target).offset().top;
			$('html,body').animate({scrollTop: ($(target).offset().top)-150}, 500);				
		});
		
		$('#shareOpen').click(function(e) {
			e.preventDefault();
			$('#shareModal').show();
			if (IDG.video.utils.deviceWidthLessThan(IDG.video.utils.deviceBreakpoints.tablet)) {
				$('#page-wrapper').addClass('has-overlay');
			}		
		});
		
		$('#closeShareModal').click(function(e) {
			$('#shareModal').hide();
			$('#page-wrapper').removeClass('has-overlay');
		});
		
		$('.direct-link input').click(function(e) {
			e.preventDefault();
			this.select();
		});
	});

	$(window).scroll(function(){
		if(IDG.video.utils.deviceWidthAtLeast(IDG.video.utils.deviceBreakpoints.tablet)) {
			var fixedTopTrigger = ($('.video-meta').offset().top - 90);
			if($(window).scrollTop() > fixedTopTrigger) {
				$('.video-nav-wrapper').addClass('fixed');
				$('#container').addClass('has-fixed-nav');
			}
			else {
				$('.video-nav-wrapper').removeClass('fixed');
				$('#container').removeClass('has-fixed-nav');
			}
		}
	});
	
	var hoverTimeout;
	$(".tippable").hover(function() {
		var tooltipElem = $(this).children('.tooltip');
		$('.tooltip').not(tooltipElem).fadeOut(); //clean up any stray tooltips except this one
		if ($(tooltipElem).is(':hidden')) {
			if(IDG.video.utils.deviceWidthAtLeast(IDG.video.utils.deviceBreakpoints.desktop)) {
				var elem = this;
				hoverTimeout = setTimeout(function(){displayTooltip(elem)},250);	
				//console.log('moused over, set timeout ' + hoverTimeout);
			}
		} else clearTimeout(hideTooltipTimeout);
	}, function(){
		//console.log('clearing timeout ' + hoverTimeout);
		clearTimeout(hoverTimeout);
		var tooltipElem = $(this).children('.tooltip');
		hideTooltipTimeout = setTimeout(function(){$(tooltipElem).fadeOut()},150);
	});

</script>


	
	













<script language="JavaScript" type="text/javascript">
	var omniture_account="computerworldcom";
	
