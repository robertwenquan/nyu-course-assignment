        <div class="content">
        	
        <script type="text/javascript" language="javascript">
    function showMoreTopics () {
        this.className = "active";
        
        $("#more_topics").animate({
            opacity: 1,
            height: "toggle"
        }, 700);
    }
    
    function hideMoreTopics () {
        this.className = "";
    
        $("#more_topics").animate({
            opacity: 0,
            height: "toggle"
        }, 200);
    }
	
	function makeActive () {
        this.className += " active";
    }
    
    function makeUnactive () {
        this.className = "";
    }
    
    $(document).ready(function(){	
    
       /*initialize more topics menu*/	
	        var moreTopicsConfig = {    
	            sensitivity: 1, // number = sensitivity threshold (must be 1 or higher)    
	            interval: 0, // number = milliseconds for onMouseOver polling interval    
	            over: showMoreTopics, // function = onMouseOver callback (REQUIRED)    
	            timeout: 500, // number = milliseconds delay before onMouseOut    
	            out: hideMoreTopics // function = onMouseOut callback (REQUIRED)
			};
			
			var hoverConfig = {    
	            sensitivity: 1, // number = sensitivity threshold (must be 1 or higher)    
	            interval: 0, // number = milliseconds for onMouseOver polling interval    
	            over: makeActive, // function = onMouseOver callback (REQUIRED)    
	            timeout: 0, // number = milliseconds delay before onMouseOut    
	            out: makeUnactive // function = onMouseOut callback (REQUIRED)
			};
			
			
		/*initialize navigation hover states*/
        $("#newsbytopic").hoverIntent(moreTopicsConfig);
        /*end more topics menu*/
		
		/*set up other calls for hover states*/
		$("#nav_home").hoverIntent(hoverConfig);
		$("#nav_wps").hoverIntent(hoverConfig);
		$("#nav_rss").hoverIntent(hoverConfig);
		$("#nav_nlsignup").hoverIntent(hoverConfig);
    
    });/*end document ready*/
    
    </script>


